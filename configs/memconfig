;; Cache Geometry for CPUs and GPUs.
;; (Summary at the end of this file)

[CacheGeometry geo-cpu-i-l1]
Sets = 512
Assoc = 1
BlockSize = 64
Latency = 1
Policy = LRU
Ports = 2

[CacheGeometry geo-cpu-d-l1]
Sets = 512
Assoc = 1
BlockSize = 64
Latency = 1
Policy = LRU
Ports = 2

[CacheGeometry geo-cpu-l2]
Sets = 1024
Assoc = 8
BlockSize = 64
Latency = 4
Policy = LRU
Ports = 4

[CacheGeometry geo-gpu-l1]
Sets = 256
Assoc = 4
BlockSize = 64
Latency = 22
Policy = LRU
Ports = 2

;; Cache Geometry L2$ for GPUs.

[CacheGeometry geo-gpu-l2]
Sets = 512
Assoc = 16
BlockSize = 64
Latency = 63
Policy = LRU
Ports = 4


